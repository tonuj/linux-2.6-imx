&i2c1 {
	clock-frequency = <100000>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c1_2>;
	status = "okay";

#if 1
	ov564x_mipi: ov564x_mipi@3c {
		compatible = "ovti,ov564x_mipi";
		reg = <0x3c>;
		clocks = <&clks 200>;
		clock-names = "csi_mclk";
        /*	pwn-gpios = <&gpio3 13 1>; */
	/*	rst-gpios = <&gpio4 10 0>; */
                pwn-gpios = <&gpio5 12 1>; //__ERROR__ yhendus olemas
                rst-gpios = <&gpio4 27 0>;//__ERROR__ yhendus olemas
                csi_id = <0>;
		mclk = <24000000>;
		mclk_source = <0>;
	};
#endif
};

&i2c2 {
	clock-frequency = <100000>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c2_2>;
	status = "okay";

	codec: tlv320aic3x@1b {
		compatible = "ti,tlv320aic3x";
		reg = <0x1b>;
		clocks = <&clks 201>;
		clock-names = "clko_clk";
		IOVDD-supply = <&reg_audio>;
		DVDD-supply = <&reg_audio>;
		AVDD-supply = <&reg_audio>;
		DRVDD-supply = <&reg_audio>;
		gpio-reset = <&gpio4 5 1>;
		gpio-cfg = <
			0x0000 /* 0:Default */
			0x0000 /* 1:Default */
			0x0013 /* 2:FN_DMICCLK */
			0x0000 /* 3:Default */
			0x8014 /* 4:FN_DMICCDAT */
			0x0000 /* 5:Default */
		>;
	};
};

&i2c3 {
	clock-frequency = <100000>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c3_3>;
	status = "okay";

	/* DS1307 RTC module */
	rtc@0x68 {
		compatible = "dallas,ds1307";
		reg = <0x68>;
	};
#if 1
        adv7180: adv7180@20 {
             compatible = "adv,adv7180";
