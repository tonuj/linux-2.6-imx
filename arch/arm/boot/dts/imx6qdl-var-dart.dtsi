        // find out which input ( /dev/input/eventX ):
        // cat /proc/bus/input/devices
        gpio_keys {
                        compatible = "gpio-keys";
                        pinctrl-names = "default";
                        pinctrl-0 = <&pinctrl_gpio_keys>;

                        //http://unix.stackexchange.com/questions/130656/how-to-get-all-my-keys-to-send-keycodes
                        photo {
                                label = "Photo";
                                linux,code = <103>; /* up */
                                gpios = <&gpio4 25 1>; /* active low */
                                gpio-key,wakeup;
                                debounce-interval = <500>;
                        };
                        stream {
                                label = "Stream";
                                linux,code = <108>; /* down */
                                gpios = <&gpio5 5 1>;/* active low */
                                gpio-key,wakeup;
                                debounce-interval = <500>;
                        };
                        record {
                                label = "Record";
                                linux,code = <105>; /* left */
                                gpios = <&gpio4 24 1>;/* active low */
                                gpio-key,wakeup;
                                debounce-interval = <500>;
                        };
        };
};

