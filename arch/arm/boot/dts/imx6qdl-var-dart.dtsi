&gpc {
	/* use ldo-bypass, u-boot will check it and configure */
	fsl,ldo-bypass = <1>;
};


#if 1
&mipi_csi {
	status = "okay";
        ipu_id = <0>;
        csi_id = <0>;
        v_channel = <0>;
	lanes = <2>;
