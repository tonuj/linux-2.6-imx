                                MX6QDL_PAD_KEY_ROW2__GPIO4_IO11         0x80000000 //GPIO_OUT_MIPICAM_PWR
                                MX6QDL_PAD_RGMII_TXC__GPIO6_IO19        0x80000000 //GPIO_OUT_ACAM_PWR
                                MX6QDL_PAD_RGMII_TX_CTL__GPIO6_IO26     0x80000000 //GPIO_IN_ACAM_ON
                                MX6QDL_PAD_RGMII_TD0__GPIO6_IO20        0x80000000 //GPIO_OUT_ACAM_RST
                                MX6QDL_PAD_DISP0_DAT0__GPIO4_IO21       0x80000000 //GPIO_OUT_IR_LED
                                MX6QDL_PAD_DISP0_DAT1__GPIO4_IO22       0x80000000 //GPIO_OUT_REC_EN
                                MX6QDL_PAD_DISP0_DAT2__GPIO4_IO23       0x80000000 //GPIO_OUT_REC_LED
                                MX6QDL_PAD_DI0_DISP_CLK__GPIO4_IO16     0x80000000 //GPIO_OUT_GSM_ONOFF
                                MX6QDL_PAD_DISP0_DAT7__GPIO4_IO28       0x80000000 //GPIO_OUT_GSM_RESET
                                MX6QDL_PAD_DISP0_DAT20__GPIO5_IO14      0x80000000 //GPIO_OUT_SHUTDOWN
                                MX6QDL_PAD_DISP0_DAT9__GPIO4_IO30       0x80000000 //GPIO_OUT_GSM_PWR


                                MX6QDL_PAD_DISP0_DAT18__GPIO5_IO12      0x80000000 // dummy
